.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}
.cu-progress {
	overflow: hidden;
	height: 28rpx;
	background-color: #ebeef5;
	display: inline-flex;
	align-items: center;
	width: 80%;
  text-align: center;
}
.tagBall{
      width: 420rpx;
      height: 300rpx;
      margin:300rpx auto;
      position: relative;
      left: 120rpx;
  }
  .IDw{
    width: 40px;
    height: 20px;
  }
  .tag{
      display: block;
      position: absolute;
      left: 0rpx;
      top: 0rpx;
      color: #000;
      text-decoration: none;
      font-size: 32rpx;
      font-family: "微软雅黑";
      font-weight: bold;
  }
  .tag:hover{border:1px solid #666;}
