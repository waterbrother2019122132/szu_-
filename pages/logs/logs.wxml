<form bindsubmit="formSubmit" bindreset="formReset">
	<view class="cu-form-group margin-top">
		<view class="title">分析内容</view>
		<input placeholder="股票代码" name="stackid"></input>
	</view>

	<view class="cu-form-group">
		<view class="title">开始日期</view>
		<picker mode="date" value="{{end_date}}" start="2000-01-01" end="2020-05-07" bindchange="sDateChange" name="start_time">
			<view class="picker">
				{{start_date}}
			</view>
		</picker>
	</view>
  <view class="cu-form-group">
		<view class="title">结束日期</view>
		<picker mode="date" value="{{end_date}}" start="2000-01-01" end="2020-05-07" bindchange="eDateChange" name="end_time">
			<view class="picker">
				{{end_date}}
			</view>
		</picker>
	</view>

   <view class="btn-area">
   <view class="btn"> 
    <button formType="submit" bindtap="" type="primary" 
    >Submit</button>
	</view>
	  
    <button formType="reset" type="warn" >Reset</button>
  </view>
</form> 
